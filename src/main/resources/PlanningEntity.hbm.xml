<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lurius.les_visiteurs.model.PlanningEntity" table="planning" schema="visiteurs">
        <id name="idPlanning">
            <column name="idPlanning" sql-type="int"/>
        </id>
        <property name="nom">
            <column name="nom" sql-type="varchar(25)" length="25"/>
        </property>
    </class>
</hibernate-mapping>